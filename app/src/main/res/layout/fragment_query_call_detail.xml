<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    style="@style/normalWindow"
    android:background="@drawable/calldata_bg"
    tools:context="com.thinkware.florida.ui.fragment.QueryCallDetailFragment">

